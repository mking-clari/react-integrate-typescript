{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": "off",
    "eol-last": "off",
    "import/first": "off",
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "max-len": "off",
    "no-shadow": "off",
    "no-trailing-spaces": "off",
    "quotes": "off",
    "react/prop-types": "off",
    "react/self-closing-comp": "off"
  }
}